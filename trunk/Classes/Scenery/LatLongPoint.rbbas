#tag ClassProtected Class LatLongPoint	#tag Method, Flags = &h0		Sub Constructor(latitude as Double, longitude as Double)		  pLatitude = latitude		  pLongitude = longitude		  		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Function getDistanceFrom(latitude as double, longitude as double) As double		  return sqrt(abs(latitude - pLatitude) ^ 2 + abs(longitude - pLongitude) ^ 2)		End Function	#tag EndMethod	#tag Property, Flags = &h0		pLatitude As Double	#tag EndProperty	#tag Property, Flags = &h0		pLongitude As Double	#tag EndPropertyEnd Class#tag EndClass